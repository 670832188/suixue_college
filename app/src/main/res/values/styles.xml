        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:textColor">@color/color_common_light_gray</item>
    </style>

    <style name="style_text_normal_gray">
        <item name="android:textSize">@dimen/text_size_normal</item>
        <item name="android:textColor">@color/color_common_light_gray</item>
    </style>

    <style name="style_text_large_gray">
        <item name="android:textSize">@dimen/text_size_large</item>
        <item name="android:textColor">@color/color_common_light_gray</item>
    </style>

    <style name="style_text_larger_gray">
