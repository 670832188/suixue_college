<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <EditText
        android:id="@+id/et_date_of_birth"
        style="@style/style_text_large_white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/margin_larger"
        android:layout_marginStart="@dimen/margin_larger"
        android:layout_marginTop="80dp"
        android:background="@null"
        android:drawableBottom="@drawable/bg_et_bottom" />

    <com.dev.kit.basemodule.view.AutoLinkStyleTextView
        android:id="@+id/tv_protocol_tip"
        style="@style/style_text_normal_white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/margin_larger"
        android:layout_marginStart="@dimen/margin_larger" />
</LinearLayout>